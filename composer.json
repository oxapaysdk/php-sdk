{
  "name": "oxapay/php-sdk",
  "description": "Official PHP SDK for OxaPay crypto payment gateway (PHP >= 8.0).",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "OxaPay",
      "email": "dev@oxapay.com"
    }
  ],
  "keywords": [
    "php",
    "oxapay",
    "crypto",
    "payments",
    "gateway",
    "sdk"
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.5|^8.0",
    "psr/log": "^1.1|^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "OxaPay\\PHP\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
